{
  "name": "coding-libs/zkteco-php",
  "description": "A package for managing ZKTeco device.",
  "keywords": [
    "laravel",
    "ZkTeco",
    "fingerprint",
    "attendances",
    "Saas"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Coding Libs",
      "email": "codinglibs4u@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "CodingLibs\\ZktecoPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "src/tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "CodingLibs\\ZktecoPhp\\ZktecoPhpServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}