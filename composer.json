{
  "name": "coding-libs/zkteco-php",
  "description": "A package for managing ZKTeco device.",
  "license": "MIT",
  "homepage": "https://github.com/coding-libs",
  "support": {
    "issues": "https://github.com/coding-libs/zkteco-php/issues",
    "source": "https://github.com/coding-libs/zkteco-php"
  },
  "keywords": [
    "laravel",
    "ZkTeco",
    "fingerprint",
    "attendances"
  ],
  "authors": [
    {
      "name": "Coding Libs",
      "email": "codinglibs4u@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.3|^10.4"
  },
  "autoload": {
    "psr-4": {
      "CodingLibs\\ZktecoPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CodingLibs\\ZktecoPhp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "CodingLibs\\ZktecoPhp\\ZktecoPhpServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}